        <script>
        $(function() {
               $(".topic").on("click",function(e){
                var topicName = e.target.childNodes[0].data;
                //alert(topic + topic.length);
				$.ajax({
    headers: {
        "Accept": "text/plain",
        "Content-Type": "application/json"
    },
    type: "GET",
    url: "/prepareForInterview/topics/"+topicName,
    dataType: "json",
    async: false,
    contentType: 'application/json',
    success: function(result) {
    	//correctAnswers = result;
       // alert(result);
    },
    error: function(result){
    	//alert(result);
    }
});

			//window.location.href = "/prepareForInterview/topics/"+topicName;
            });
        });

        
        </script>

